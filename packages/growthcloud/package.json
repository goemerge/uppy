{
  "name": "uppy",
  "description": "Extensible JavaScript file upload widget with support for drag&drop, resumable uploads, previews, restrictions, file processing/encoding, remote providers like Instagram, Dropbox, Google Drive, S3 and more :dog:",
  "version": "3.3.1",
  "license": "MIT",
  "main": "index.mjs",
  "module": "index.mjs",
  "unpkg": "dist/uppy.min.js",
  "style": "dist/uppy.min.css",
  "types": "types/index.d.ts",
  "keywords": [
    "file uploader",
    "drag-drop",
    "progress",
    "preview",
    "resumable uploads",
    "xhr",
    "tus",
    "s3",
    "google drive",
    "dropbox",
    "box",
    "webcam"
  ],
  "homepage": "https://uppy.io",
  "bugs": {
    "url": "https://github.com/goemerge/uppy/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/goemerge/uppy.git"
  },
  "dependencies": {
    "@growthcloud/audio": "workspace:^",
    "@growthcloud/aws-s3": "workspace:^",
    "@growthcloud/aws-s3-multipart": "workspace:^",
    "@growthcloud/box": "workspace:^",
    "@growthcloud/companion-client": "workspace:^",
    "@growthcloud/compressor": "workspace:^",
    "@growthcloud/core": "workspace:^",
    "@growthcloud/dashboard": "workspace:^",
    "@growthcloud/drag-drop": "workspace:^",
    "@growthcloud/drop-target": "workspace:^",
    "@growthcloud/dropbox": "workspace:^",
    "@growthcloud/facebook": "workspace:^",
    "@growthcloud/file-input": "workspace:^",
    "@growthcloud/form": "workspace:^",
    "@growthcloud/golden-retriever": "workspace:^",
    "@growthcloud/google-drive": "workspace:^",
    "@growthcloud/image-editor": "workspace:^",
    "@growthcloud/informer": "workspace:^",
    "@growthcloud/instagram": "workspace:^",
    "@growthcloud/onedrive": "workspace:^",
    "@growthcloud/progress-bar": "workspace:^",
    "@growthcloud/provider-views": "workspace:^",
    "@growthcloud/redux-dev-tools": "workspace:^",
    "@growthcloud/remote-sources": "workspace:^",
    "@growthcloud/screen-capture": "workspace:^",
    "@growthcloud/status-bar": "workspace:^",
    "@growthcloud/store-default": "workspace:^",
    "@growthcloud/store-redux": "workspace:^",
    "@growthcloud/thumbnail-generator": "workspace:^",
    "@growthcloud/transloadit": "workspace:^",
    "@growthcloud/tus": "workspace:^",
    "@growthcloud/unsplash": "workspace:^",
    "@growthcloud/url": "workspace:^",
    "@growthcloud/webcam": "workspace:^",
    "@growthcloud/xhr-upload": "workspace:^",
    "@growthcloud/zoom": "workspace:^"
  },
  "devDependencies": {
    "abortcontroller-polyfill": "^1.7.3",
    "core-js": "~3.24.0",
    "md-gum-polyfill": "^1.0.0",
    "regenerator-runtime": "0.13.9",
    "resize-observer-polyfill": "^1.5.1",
    "whatwg-fetch": "^3.6.2"
